{
  "name": "taas-api-server",
  "version": "0.0.1",
  "description": "The TaaS Web API Server",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=\"test\" nyc --reporter=html --reporter=text --report-dir=../reports/backend-coverage mocha test/*/ --delay --reporter mochawesome --reporter-options reportDir=../reports/backend-test,reportFilename=index; rm -rf .nyc_output",
    "api-test": "NODE_ENV=\"test\" nyc --reporter=html --reporter=text --report-dir=../reports/api-test/backend-coverage mocha test/api-test/ --delay --reporter mochawesome --reporter-options reportDir=../reports/api-test/backend-test,reportFilename=index; rm -rf .nyc_output",
    "unit-test": "NODE_ENV=\"test\" nyc --reporter=html --reporter=text --report-dir=../reports/unit-test/backend-coverage mocha test/unit-test/ --reporter mochawesome --reporter-options reportDir=../reports/unit-test/backend-test,reportFilename=index; rm -rf .nyc_output",
    "testprod": "nyc --reporter=html --reporter=text --report-dir=../reports/backend-coverage mocha --delay --reporter mochawesome --reporter-options reportDir=../reports/backend-test,reportFilename=index; rm -rf .nyc_output",
    "start": "node index",
    "eslint": "eslint --ignore-pattern 'test/' --ext .js ./ -f node_modules/eslint-html-reporter/reporter.js -o ../reports/backend-eslint/index.html",
    "daemon": "pm2 start index.js --watch --name=\"taas-api\"",
    "forever": "supervisor index"
  },
  "keywords": [
    "taas",
    "test"
  ],
  "author": "Shanghai QE",
  "license": "Apache-2.0",
  "dependencies": {
    "@slack/client": "^3.14.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cron-parser": "^2.5.0",
    "diff-json": "^0.1.11",
    "dockerode": "^2.5.3",
    "express": "^4.16.3",
    "fs-extra": "^4.0.3",
    "glob": "^7.1.2",
    "hasha": "^3.0.0",
    "hoek": "^5.0.3",
    "into-stream": "^3.1.0",
    "js-yaml": "^3.12.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.10",
    "moment-timezone": "^0.5.14",
    "mongo-key-escape": "0.0.1",
    "mongoose": "^4.13.6",
    "mongoose-count-and-find": "^1.0.0",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-schedule-tz": "^1.2.1-4",
    "pm2": "^2.8.0",
    "queue": "^4.4.2",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "socket.io": "^2.1.0",
    "supervisor": "^0.12.0",
    "tree-kill": "^1.2.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.12.1",
    "eslint-html-reporter": "^0.5.2",
    "mocha": "^5.2.0",
    "mochawesome": "^3.0.2",
    "nyc": "^11.4.0",
    "sinon": "^6.1.3",
    "sinon-mongoose": "^2.1.1",
    "sonarqube-scanner": "^2.0.2",
    "supertest": "^3.1.0"
  }
}
